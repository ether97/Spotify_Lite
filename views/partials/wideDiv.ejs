<div style="width: 100%; display: grid; grid-template-columns: 2fr 1fr 2fr 0.5fr;align-items: center; height: 50px; margin: 20px 0px 20px 0px;">
  <div style="display: flex; align-items: center;  gap: 20px; ">
    <p style="color: gray"><%= index %></p>
    <div style="background-image: url('../../public/images/<%= picture %>'); height: 50px; width: 50px; background-position: center; background-size: cover; background-repeat: no-repeat;"></div>
    <div style="display: flex; flex-direction: column; height: 50px;  justify-content: space-between;">
      <h4 style="color: white"><%= title %></h4>
      <p style="color: gray;"> <%= artist %></p>
    </div>
  </div>
  <div style="display: flex;  height: 50px; align-items: center; ">

    <p style="color: gray;"><%= album %></p>

  </div>
  <div style="display: flex; ; height: 50px;  align-items: center; ">
    <p style="color: gray;"><%= date %></p>

  </div>
  <div style=" display: flex; gap: 40px; ">
    <p style="color: gray"><%= runtime %></p>
  </div>
</div>